  "main": "index.js",
  "scripts": {
    "seed": "node app/seed/seed.js",
    "lint": "node node_modules/eslint/bin/eslint.js app",
    "start": "node app/app.js",
    "test": "nyc mocha tests/**/*.test.js --test  --recursive --exit",
